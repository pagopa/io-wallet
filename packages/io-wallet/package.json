{
  "name": "@io-wallet/io-wallet",
  "version": "0.0.1",
  "repository": "https://github.com/pagopa/io-wallet",
  "exports": {
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint --fix \"src/**\"",
    "build": "tsc"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@pagopa/handler-kit": "^1.1.0",
    "@pagopa/logger": "^1.0.1",
    "fp-ts": "^2.16.6"
  },
  "devDependencies": {
    "@pagopa/eslint-config": "^3.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitest/coverage-c8": "^0.33.0",
    "eslint": "^8.28.0",
    "prettier": "2.7.1",
    "tsup": "^7.2.0",
    "typescript": "^4.8.4",
    "vitest": "^1.2.2"
  }
}
